{
  "name": "react-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install:frontend": "cd react-project && npm install",
    "install:backend": "cd server && npm install",
    "start:frontend": "cd react-project && npm start",
    "start:backend": "cd server && npm start",
    "eject": "npm eject",
    "test": "cypress run",
    "cy:run": "cypress run",
    "run-cypress-tests": "cypress run --spec \"cypress/e2e/*.spec.cy.js\"",
    "cy:open": "cypress open",

    "start:all": "npm run start:frontend && npm run start:backend ",
    "cy:open:with:servers": "start-server-and-test start:frontend 3000 start:backend 3000 cy:open",
    "test:api:with:servers": "start-server-and-test  start:frontend 3000 start:backend 3000 run-cypress-tests"
    },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },"dependencies": {
    "@testing-library/cypress": "^9.0.0",
    "cypress": "^12.7.0",
    "mocha": "^10.2.0",
    "start-server-and-test": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "cypress": "^12.7.0",
    "mocha": "^10.2.0",
    "start-server-and-test": "^2.0.0"
  },
  "description": "Completed part of the Dashboard Page",
  "author": "",
  "license": "ISC"
}
